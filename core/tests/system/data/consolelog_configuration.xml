<?xml version="1.0" encoding="UTF-8"?>
<core>
    <log path="C:\temp\dtaf_logs\" />
    <host>
        <logging format="jUnit"/>
        <root>C:\workspace</root>
        <os name="Windows" version="10">
            <kernel version="10.0.14393" />
        </os>
    </host>
    <suts>
        <sut ip="10.13.168.111">
            <platform type="commercial" boardname="WolfPass" name="purley" />
            <silicon>
                <cpu>
                    <family>SKX</family>
                </cpu>
                <pch>
                    <family>LBG</family>
                </pch>
            </silicon>
            <providers>
                <console/>
                <sut_os name="Linux" subtype="RHEL" version="7.6" kernel="3.10">
                    <shutdown_delay>5.0</shutdown_delay>
                    <driver>
                        <ssh>
                            <credentials user="root" password="password"/>
                            <ipv4>10.190.191.121</ipv4>
                        </ssh>
                    </driver>
                </sut_os>
                <sut_os name="Windows" subtype="Win" version="10" kernel="3.10">
                    <shutdown_delay>5.0</shutdown_delay>
                    <driver>
                        <com>
                            <baudrate>115200</baudrate>
                            <port>COM4</port>
                        </com>
                    </driver>
                </sut_os>
                <sut_os name="Linux" subtype="CentOS" version="7.6" kernel="3.10">
                    <shutdown_delay>5.0</shutdown_delay>
                    <driver>
                        <socket>
                            <ipv4>10.13.168.111</ipv4>
                        </socket>
                    </driver>
                </sut_os>
                <uefi_shell>
                    <driver>
                        <sol>
                            <address>0.0.0.0</address>
                            <port>1101</port>
                            <timeout>5</timeout>
                            <credentials user="xxxx" password="xxxxx"/>
                        </sol>
                    </driver>
                </uefi_shell>
                <console_log id="1">
                    <runwith/>
                    <logpath>logs</logpath>
                    <driver>
                        <com>
                            <baudrate>115200</baudrate>
                            <port>COM1</port>
                            <timeout>5</timeout>
                        </com>
                    </driver>
                </console_log>
                <console_log id="2">
                    <runwith/>
                    <logpath>logs</logpath>
                    <driver>
                        <com>
                            <baudrate>115200</baudrate>
                            <port>COM2</port>
                            <timeout>5</timeout>
                        </com>
                    </driver>
                </console_log>
                <bios_setupmenu>
                    <driver>
                        <com>
                            <baudrate>115200</baudrate>
                            <port>COM100</port>
                            <timeout>5</timeout>
                        </com>
                    </driver>
                    <efishell_entry select_item="Launch EFI Shell">
                        <path>
                            <node>Setup Menu</node>
                            <node>Boot Manager</node>
                        </path>
                    </efishell_entry>
                    <continue select_item="Continue"/>
                    <reset press_key="\\33R\\33r\\33R" parse="False"/>
                </bios_setupmenu>
                <bios_bootmenu>
                    <driver>
                        <com>
                            <baudrate>115200</baudrate>
                            <port>COM100</port>
                            <timeout>5</timeout>
                        </com>
                    </driver>
                    <efishell_entry select_item="Launch EFI Shell"/>
                </bios_bootmenu>
                <bios>
					    <driver>
						    <xmlcli>
								<sutospath>"/opt/APP/xmlcli/"</sutospath>
								<bios_cfgfilepath>C:\biosxml\</bios_cfgfilepath>
								<bios_cfgfilename>neoncity.cfg</bios_cfgfilename>
								<ip>10.190.179.22</ip>
								<user>root</user>
								<password>password</password>
							</xmlcli>
						</driver>					
                </bios>
                <physical_control>
                    <timeout>
                        <usbswitch>4</usbswitch>
                        <clearcmos>3</clearcmos>>
                    </timeout>
                </physical_control>
                <pi>
                    <driver>
                        <raspberry>
                            <ip>10.190.179.119</ip>
                            <port>80</port>
                            <proxy>http://child-prc.intel.com:913</proxy>
                        </raspberry>
                    </driver>
                </pi>
                <ac>
                   <driver>
                        <rsc2>
                        </rsc2>
                    </driver>
                    <timeout>
                        <power_on>5</power_on>
                        <power_off>20</power_off>
                    </timeout>
                </ac>
