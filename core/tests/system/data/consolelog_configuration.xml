<!--                            </dllpath>-->
<!--                        </banino>-->
<!--                    </driver>-->
<!--                    <timeout>-->
<!--                        <power_on>5</power_on>-->
<!--                        <power_off>20</power_off>-->
<!--                    </timeout>-->
<!--                </dc>-->
                <dc>
                   <driver>
                        <rsc2>
                        </rsc2>
                    </driver>
                    <timeout>
                        <power_on>5</power_on>
                        <power_off>20</power_off>
                    </timeout>
                </dc>
              
                <silicon_debug>
                    <driver>
                        <xdp type="ITP" />
                    </driver>
                </silicon_debug>
                <silicon_reg>
                    <driver>
                        <cscripts>
                            <debugger_interface_type>ITP</debugger_interface_type>
                            <silicon>
                                <cpu_family>SKX</cpu_family>
                                <pch_family>LBG</pch_family>
                            </silicon>
                        </cscripts>
                    </driver>
                </silicon_reg>
            </providers>
        </sut>
    </suts>
</core>
